    <widget class="QTextEdit" name="uiAboutText">
     <property name="enabled">
      <bool>true</bool>
     </property>
     <property name="geometry">
      <rect>
       <x>90</x>
       <y>250</y>
       <width>591</width>
       <height>181</height>
